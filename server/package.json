  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "echo": "echo $npm_package_config_memory",
    "test": "jest --runInBand --watchAll",
    "debug": "nodemon ./dist/server.js",
    "start": "node ./dist/server.js",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint -c ./tslint.json --exclude **/*.d.ts --exclude ./node_modules --project . --fix **/*.tsx",
    "local": "cross-env NODE_ENV=local NODE_OPTIONS=--max_old_space_size=$npm_package_config_memory webpack --progress --hide-modules",
    "stg": "cross-env NODE_ENV=staging NODE_OPTIONS=--max_old_space_size=$npm_package_config_memory webpack --progress --hide-modules",
    "prod": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=$npm_package_config_memory webpack --progress --hide-modules"
